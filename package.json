{
  "name": "muttley-admin-front",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "newSecret": "sh ./_scripts/secret/generate-new-secret-file.sh",
    "newSecretIfNotExists": "sh ./_scripts/secret/generate-secret-file-if-not-exists.sh",
    "start": "npm run newSecretIfNotExists && npm run clean && ng serve --host 0.0.0.0 --proxy-config proxy.conf.js --port 4250",
    "clean": "rm -rf package-lock.json .dist/ dist/",
    "updatepackage": "ncu && ncu -u",
    "reinstall": "rm -rf node_modules/ && npm run clean && npm install ",
    "build": "ng build",
    "buildProd": "npm run newSecret && npm run clean && ng build --prod=true --progress=true --aot=true --output-path=dist",
    "buildTest": "npm run newSecret && npm run clean && ng build --configuration dev --progress=true --aot=true --output-path=dist",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.5",
    "@angular/cdk": "^12.0.5",
    "@angular/common": "~12.0.5",
    "@angular/compiler": "~12.0.5",
    "@angular/core": "~12.0.5",
    "@angular/forms": "~12.0.5",
    "@angular/material": "^12.0.5",
    "@angular/platform-browser": "~12.0.5",
    "@angular/platform-browser-dynamic": "~12.0.5",
    "@angular/router": "~12.0.5",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.5",
    "@angular/cli": "~12.0.5",
    "@angular/compiler-cli": "~12.0.5",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "npm-check-updates": "^11.7.1",
    "tslint": "6.1.3",
    "typescript": "~4.2.3"
  }
}
